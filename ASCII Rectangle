#include<stdio.h>

void print_ascii_rectangle(char s, int w, int h)
{
    for (int i=0; i<h; i++)
    {
        for (int j=0; j<w; j++)
        {
        printf("%c", s);
        }
        printf("\n");
    }
}

int main()
{
    char s;
    int w, h;
    printf("Please enter an ASCII symbol:\n");
    scanf(" %c", &s);
    printf("Please enter the width:\n");
    scanf(" %d", &w);
    printf("Please enter the height:\n");
    scanf(" %d", &h);
    
    print_ascii_rectangle(s, w, h);
}
