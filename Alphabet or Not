#include <stdio.h>

char is_letter(char input)
{
    if ((input >= 'A' && input <= 'Z') || (input >= 'a' && input <= 'z'))
    {
        return 1;
    }
    else
    {
        return 0;
    }
}

int main()

{
    
    char input;
    scanf(" %c", &input);
	printf("> %d", is_letter(input));
	
	return 0;

}
