#include <stdio.h>

int is_digit(char c)
{
    if (c>='0' && c<='9')
    {
        return 1;
    }
    else 
    {
        return 0;
    }
}

int main()

{
    
    char character;
    
    scanf(" %c", &character);
    
    int result = is_digit(character);
    
    printf("> %d", result);
    
    return 0;
    
}
