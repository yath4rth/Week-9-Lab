#include <stdio.h>

char is_vowel(char c)
{
    if (c=='a' || c=='e' || c=='i' || c=='o' || c=='u' || c=='A' || c=='E' || c=='I' || c=='O' || c=='U')
    {
        return 1;
    }
    else 
    {
        return 0;
    }
}

int main()

{
    
    char c;
    scanf(" %c", &c);
    
	printf("> %d", is_vowel(c));
	
	return 0;

}
